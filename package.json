{
    "name": "mui-feedback-dialog",
    "version": "2.3.2",
    "homepage": "https://github.com/fochlac/mui-feedback-dialog",
    "description": "A customizeable React feedback form with optional screenshot via screen capture and canvas editor based on material-ui.",
    "main": "dist/mui-feedback-dialog.js",
    "directories": {
        "lib": "lib"
    },
    "scripts": {
        "test": "jest",
        "start": "vite dev",
        "build": "rm -rf ./dist/ && tsc && vite build",
        "lint": "eslint ./lib/**/*.tsx ./lib/**/*.ts"
    },
    "author": "Florian Riedel",
    "license": "MIT",
    "jest": {
        "testRegex": "/__tests__/.*-test.[jt]sx?$",
        "testPathIgnorePatterns": [
            "node_modules",
            "dist"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run build"
        }
    },
    "peerDependencies": {
        "@mui/icons-material": "^5.10.14",
        "@mui/material": "^5.10.13",
        "@mui/system": "^5.10.13",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@types/jest": "^29.2.3",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.8",
        "@types/react-helmet": "^6.1.5",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "@typescript-eslint/typescript-estree": "^5.43.0",
        "@vitejs/plugin-react": "^4.3.1",
        "eslint": "^8.27.0",
        "eslint-config-recommended": "^4.1.0",
        "eslint-plugin-jest": "^27.1.5",
        "eslint-plugin-promise": "^*",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-standard": "^5.0.0",
        "modern-screenshot": "^4.4.39",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "jest-junit": "^14.0.1",
        "npm-run-all": "^4.1.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^4.9.3",
        "vite": "^5.4.7",
        "vite-plugin-svgr": "^2.2.2",
        "vite-tsconfig-paths": "^3.5.2"
    }
}
