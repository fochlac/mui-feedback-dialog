{
    "name": "mui-feedback-dialog",
    "version": "1.3.2",
    "homepage": "https://github.com/fochlac/mui-feedback-dialog",
    "description": "A customizeable React feedback form with optional screenshot via screen capture and canvas editor based on material-ui.",
    "main": "dist/index.js",
    "directories": {
        "lib": "lib"
    },
    "scripts": {
        "test": "jest",
        "start": "webpack-dev-server --hot --config webpack.config.showcase.js",
        "build": "rm -rf ./dist/ && webpack -p --config webpack.config.js",
        "lint": "eslint ./lib/**/*.tsx ./lib/**/*.ts"
    },
    "author": "Florian Riedel",
    "license": "MIT",
    "jest": {
        "testRegex": "/__tests__/.*-test.[jt]sx?$",
        "testPathIgnorePatterns": [
            "node_modules",
            "dist"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run build",
            "pre-push": "npm run test"
        }
    },
    "peerDependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/node": "^7.10.5",
        "@babel/plugin-proposal-async-generator-functions": "^7.10.5",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
        "@babel/plugin-proposal-optional-catch-binding": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@types/jest": "^26.0.4",
        "@types/react": "^16.9.43",
        "@types/react-dom": "^16.9.8",
        "@types/react-helmet": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^3.6.1",
        "@typescript-eslint/parser": "^3.6.1",
        "@typescript-eslint/typescript-estree": "^3.6.1",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.1.0",
        "babel-loader": "^8.1.0",
        "clsx": "^1.1.1",
        "core-js": "^3.6.5",
        "css-loader": "^3.6.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.5.0",
        "eslint": "^7.4.0",
        "eslint-config-recommended": "^4.1.0",
        "eslint-plugin-jest": "^23.18.0",
        "eslint-plugin-promise": "^*",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-standard": "^4.0.1",
        "html-webpack-plugin": "^4.3.0",
        "husky": "^4.2.5",
        "jest": "^26.1.0",
        "jest-junit": "^11.0.0",
        "npm-run-all": "^4.1.5",
        "null-loader": "^4.0.0",
        "postcss-loader": "^3.0.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "style-loader": "^1.2.1",
        "ts-loader": "^7.0.1",
        "typescript": "^3.9.6",
        "webpack": "^4.43.0",
        "webpack-bundle-size-analyzer": "^3.1.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.10.3",
        "webpack-node-externals": "^2",
        "dom-to-image": "^2.6.0"
    }
}
